{
  "name": "elmnotate",
  "version": "1.0.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "elm:make": "elm make Main.elm --output elm.js || true",
    "elm:watch": "chokidar '**/*.elm' -c 'npm run elm:make'",
    "start": "NODE_ENV=${NODE_ENV-development} electron main.js",
    "watch": "npm run elm:watch & npm run start & wait",
    "package": "electron-packager --electron-version=1.8.6 .",
    "package-mac": "electron-packager --electron-version=1.8.6 --platform=darwin .",
    "package-win": "electron-packager --electron-version=1.8.6 --platform=win32 .",
    "package-linux": "electron-packager --electron-version=1.8.6 --platform=linux ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "electron-packager": "^12.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.0"
  }
}
